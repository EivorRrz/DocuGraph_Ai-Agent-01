# Server Configuration
PORT=3000
NODE_ENV=development

# MongoDB Configuration
MONGODB_URI=mongodb://localhost:27017/document-graph-pipeline

# Neo4j Configuration
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=your-password-here

# LlamaParse Configuration (optional)
LLAMAPARSE_API_KEY=your-llamaparse-api-key

# LLM Configuration - Schema Extraction
OLLAMA_BASE_URL=http://localhost:11434
SCHEMA_EXTRACTION_MODEL=deepseek-r1-distill
HF_API_KEY=your-huggingface-api-key
SCHEMA_MODEL_PROVIDER=ollama

# Text2Cypher Model Configuration
TEXT2CYPHER_MODEL_HF=tomasonjo/text2cypher-demo-16bit
TEXT2CYPHER_MODEL_OLLAMA=text2cypher-demo-4bit-gguf
CYPHER_MODEL_PROVIDER=huggingface

# Chunking Configuration
CHUNK_SIZE_WORDS=1000
CHUNK_OVERLAP_WORDS=100

# Logging
LOG_LEVEL=info
